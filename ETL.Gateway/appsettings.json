{
  "ReverseProxy": {
    "Routes": {
      "serviceRoute": {
        "ClusterId": "serviceCluster",
        "Match": {
          "Path": "{**catch-all}"
        }
      }
    },
    "Clusters": {
      "serviceCluster": {
        "Destinations": {
          "serviceDestination": {
            "Address": "http://localhost:5092/"
          }
        }
      }
    }
  }
}